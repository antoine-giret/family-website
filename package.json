{
  "name": "family-website",
  "version": "1.0.0",
  "private": true,
  "description": "family-website",
  "author": "Antoine Giret",
  "keywords": [
    "gatsby"
  ],
  "scripts": {
    "preinstall": "cp -n .env.sample .env | true",
    "develop": "gatsby develop",
    "start": "gatsby develop",
    "build": "gatsby build",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "lint": "eslint \"src/**/*.{js,jsx,ts,tsx,json}\"",
    "lint:fix": "eslint --fix \"src/**/*.{js,jsx,ts,tsx,json}\"",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,css,md,json}\" --config ./.prettierrc",
    "prepare": "husky install"
  },
  "dependencies": {
    "gatsby": "5.5.0",
    "gatsby-omni-font-loader": "2.0.2",
    "gatsby-plugin-graphql-codegen": "3.1.1",
    "gatsby-plugin-image": "3.5.0",
    "gatsby-plugin-sharp": "5.5.0",
    "gatsby-plugin-theme-ui": "0.15.4",
    "gatsby-source-filesystem": "5.5.0",
    "gatsby-source-prismic": "5.2.10",
    "gatsby-transformer-sharp": "5.5.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-helmet": "6.1.0",
    "theme-ui": "0.15.4"
  },
  "devDependencies": {
    "@commitlint/cli": "17.4.2",
    "@commitlint/config-conventional": "17.4.2",
    "@emotion/react": "11.10.5",
    "@mdx-js/react": "2.2.1",
    "@theme-ui/mdx": "0.15.4",
    "@types/node": "18.11.18",
    "@types/react": "18.0.27",
    "@types/react-dom": "18.0.10",
    "@typescript-eslint/eslint-plugin": "5.50.0",
    "@typescript-eslint/parser": "5.50.0",
    "eslint": "8.33.0",
    "eslint-config-prettier": "8.6.0",
    "eslint-import-resolver-typescript": "3.5.3",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-react": "7.32.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "husky": "8.0.0",
    "lint-staged": "13.1.0",
    "prettier": "2.8.3",
    "typescript": "4.9.4"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": [
      "npx prettier --write \"src/**/*.{js,jsx,ts,tsx,css,md,json}\" --config ./.prettierrc",
      "npx eslint --fix \"src/**/*.{js,jsx,ts,tsx,json}\""
    ]
  },
  "engines": {
    "node": ">= 18.13.0",
    "npm": "please-use-yarn",
    "yarn": ">= 1.22.15"
  }
}
